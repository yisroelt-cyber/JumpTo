<?xml version='1.0' encoding='UTF-8'?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>3a8dbcc6-5e5c-4735-8362-fb7ac432a067</Id>
  <Version>1.0.0.37</Version>
  <ProviderName>JumpTo</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="JumpTo"/>
  <Description DefaultValue="Jump quickly to any worksheet."/>
  <IconUrl DefaultValue="https://yisroelt-cyber.github.io/JumpTo/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://yisroelt-cyber.github.io/JumpTo/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://yisroelt-cyber.github.io/JumpTo/support"/>

  <AppDomains>
    <AppDomain>https://yisroelt-cyber.github.io</AppDomain>
  </AppDomains>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="ExcelApi" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://yisroelt-cyber.github.io/JumpTo/taskpane.html?v=37"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Requirements>
      <bt:Sets DefaultMinVersion="1.1">
        <bt:Set Name="ExcelApi" MinVersion="1.1"/>
      </bt:Sets>
    </Requirements>

    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>

          
          <Runtimes>
            <Runtime resid="Taskpane.Url" lifetime="long"/>
          </Runtimes>

          <Taskpane id="JumpTo.Taskpane">
            <SourceLocation resid="Taskpane.Url"/>
          </Taskpane>

<ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabAddIns">
              <Group id="JumpTo.Group.AddIns">
                <Label resid="JumpTo.GroupLabel"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>

                <Control xsi:type="Button" id="JumpTo.OpenDialogButton.AddIns">
                  <Label resid="JumpTo.ButtonLabel"/>
                  <Supertip>
                    <Title resid="JumpTo.ButtonLabel"/>
                    <Description resid="JumpTo.ButtonTooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>openJumpDialog</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="JumpTo.ShowTaskpaneButton.AddIns">
                  <Label resid="JumpTo.TaskpaneButtonLabel"/>
                  <Supertip>
                    <Title resid="JumpTo.TaskpaneButtonLabel"/>
                    <Description resid="JumpTo.TaskpaneButtonTooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>openJumpDialog</FunctionName>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>

        </DesktopFormFactor>
      </Host>
    </Hosts>

    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/assets/icon-80.png"/>
      </bt:Images>

      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/commands.html?v=37"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/taskpane.html?v=37"/>
                <bt:Url id="TaskpaneShim.Url" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/taskpane.html?shim=1&amp;v=36"/>
<bt:Url id="Dialog.Url" DefaultValue="https://yisroelt-cyber.github.io/JumpTo/dialog.html?v=37"/>
      </bt:Urls>

      <bt:ShortStrings>
        <bt:String id="JumpTo.GroupLabel" DefaultValue="JumpTo"/>
        <bt:String id="JumpTo.ButtonLabel" DefaultValue="JumpTo"/>
        <bt:String id="JumpTo.TaskpaneButtonLabel" DefaultValue="Open Taskpane (Dev)"/>
      </bt:ShortStrings>

      <bt:LongStrings>
        <bt:String id="JumpTo.ButtonTooltip" DefaultValue="Open a small search dialog to jump to a worksheet."/>
        <bt:String id="JumpTo.TaskpaneButtonTooltip" DefaultValue="(Dev) Open the taskpane launcher. Not used in normal UX."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
